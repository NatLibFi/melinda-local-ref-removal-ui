{
  "presets": ["es2015", "react", "stage-2"],
  "env": {
    "test": {
      "plugins": ["rewire"]
    },
    "production": {
      "plugins": [
        ["module-resolver", {
          "root": ["."],
          "alias": {
            "shared": "./build/shared"
          }
        }]
      ]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "server": "@natlibfi/melinda-ui-commons/dist/server/",
        "commons": "@natlibfi/melinda-ui-commons/dist/frontend/js/",
        "shared": "./shared"
      }
    }]
  ]
}
